<h1>Example App: Home</h1>

<div class="table-responsive">
    <h3>Vista productos</h3>
    <table class="table table-dark">
        <tr style="color: yellow;">
            <th>Nombre</th>
            <th>Precio</th>
            <th>URL imagen</th>
        </tr>
        {{#each productos}}
        <tr>
            <td>
                {{nombre}}
            </td>
            <td>
                {{precio}}
            </td>
            <td>
                <img src={{url}} alt="Imagen del producto" width="100px">
            </td>
        </tr>
        {{/each}}
    </table>

    <button type="button" class="btn btn-primary" onclick="window.location.href='/'">Volver al formulario</button>
</div>




